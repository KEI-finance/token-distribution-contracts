{
  "name": "@kei.fi/contracts-template",
  "version": "0.0.1",
  "main": "index.js",
  "author": "KEI finance",
  "license": "UNLICENSED",
  "scripts": {
    "deploy": "forge script script/Deploy.s.sol:DeployScript --broadcast --verify -vvvv --rpc-url",
    "typechain:clean": "rm -rf typechain",
    "typechain:v5": "typechain --target ethers-v6 --out-dir typechain-v6 \"./out/**/*.json\" --show-stack-traces",
    "typechain:v6": "typechain --target ethers-v5 --out-dir typechain-v5 \"./out/**/*.json\" --show-stack-traces",
    "typechain": "pnpm typechain:clean && pnpm typechain:v5 && pnpm typechain:v6",
    "prepublish": "forge install && forge build && pnpm typechain && forge-utils deployments"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/ethers-v6": "^0.5.1",
    "forge-utils": "^0.0.13",
    "typechain": "^8.3.2"
  },
  "peerDependencies": {
    "ethers": ">=5.0.0"
  }
}
